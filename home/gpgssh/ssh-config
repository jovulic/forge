Host cirrus.lan
  User root

Host 192.168.1.1
  User root

Host flint.lan
  User root
  PubkeyAcceptedAlgorithms +ssh-rsa
  HostkeyAlgorithms +ssh-rsa

Host 192.168.1.2
  User root
  PubkeyAcceptedAlgorithms +ssh-rsa
  HostkeyAlgorithms +ssh-rsa

Host localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host *
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
